    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "program": "${workspaceFolder}/src/index.ts",
            "preLaunchTask": "tsc: build - tsconfig.json",
            "outFiles": [
                "${workspaceFolder}/js/**/*.js"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch substitution download",
            "program": "${workspaceFolder}/src/substitution_plan/sp_download.ts",
            "preLaunchTask": "tsc: build - tsconfig.json",
            "outFiles": [
                "${workspaceFolder}/js/**/*.js"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch timetable download",
            "program": "${workspaceFolder}/src/timetable/tt_download.ts",
            "preLaunchTask": "tsc: build - tsconfig.json",
            "outFiles": [
                "${workspaceFolder}/js/**/*.js"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch calendar download",
            "program": "${workspaceFolder}/src/calendar/calendar_download.ts",
            "preLaunchTask": "tsc: build - tsconfig.json",
            "outFiles": [
                "${workspaceFolder}/js/**/*.js"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch cafetoria download",
            "program": "${workspaceFolder}/src/cafetoria/cafetoria_download.ts",
            "preLaunchTask": "tsc: build - tsconfig.json",
            "outFiles": [
                "${workspaceFolder}/js/**/*.js"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch teachers download",
            "program": "${workspaceFolder}/src/teachers/teachers_download.ts",
            "preLaunchTask": "tsc: build - tsconfig.json",
            "outFiles": [
                "${workspaceFolder}/js/**/*.js"
            ]
        },
        {
            "type": "node",
            "name": "vscode-jest-tests",
            "request": "launch",
            "program": "${workspaceFolder}/node_modules/jest/bin/jest",
            "args": [
                "--runInBand"
            ],
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen",
            "disableOptimisticBPs": true
        }
    ]
}